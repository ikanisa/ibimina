{
  "packageId": "rw.gov.ikanisa.ibimina.client",
  "host": "client.ibimina.ikanisa.gov.rw",
  "name": "Ibimina Client",
  "launcherName": "Ibimina",
  "display": "standalone",
  "themeColor": "#0b1020",
  "navigationColor": "#0b1020",
  "backgroundColor": "#ffffff",
  "enableNotifications": true,
  "startUrl": "/",
  "iconUrl": "https://client.ibimina.ikanisa.gov.rw/icons/icon-512.png",
  "maskableIconUrl": "https://client.ibimina.ikanisa.gov.rw/icons/icon-512.png",
  "monochromeIconUrl": "https://client.ibimina.ikanisa.gov.rw/icons/icon-512.png",
  "splashScreenFadeOutDuration": 300,
  "signingKey": {
    "path": "./android.keystore",
    "alias": "ibimina-client"
  },
  "appVersionName": "1.0.0",
  "appVersionCode": 1,
  "shortcuts": [
    {
      "name": "View Contributions",
      "short_name": "Contributions",
      "url": "/contributions",
      "icon": "https://client.ibimina.ikanisa.gov.rw/icons/icon-192.png"
    },
    {
      "name": "Request Loan",
      "short_name": "Loan",
      "url": "/loans/new",
      "icon": "https://client.ibimina.ikanisa.gov.rw/icons/icon-192.png"
    }
  ],
  "generatorApp": "bubblewrap-cli",
  "webManifestUrl": "https://client.ibimina.ikanisa.gov.rw/manifest.json",
  "fallbackType": "customtabs",
  "features": {
    "locationDelegation": {
      "enabled": false
    },
    "playBilling": {
      "enabled": false
    }
  },
  "alphaDependencies": {
    "enabled": false
  },
  "enableSiteSettingsShortcut": true,
  "isChromeOSOnly": false,
  "isMetaQuest": false,
  "minSdkVersion": 21,
  "targetSdkVersion": 34
}
