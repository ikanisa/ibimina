{
  "repo": "ibimina",
  "packageManager": {
    "name": "pnpm",
    "lockfile": "pnpm-lock.yaml",
    "detectedVersion": "10.5.2"
  },
  "node": {
    "engines": ">=18.18.0",
    "preferredVersion": "18",
    "localVersion": "v20.19.4"
  },
  "monorepo": false,
  "apps": [
    {
      "name": "web",
      "path": ".",
      "framework": "nextjs",
      "description": "Next.js 15 App Router frontend with API routes and Supabase integrations.",
      "commands": {
        "dev": "pnpm dev",
        "build": "pnpm build",
        "start": "pnpm start",
        "lint": "pnpm lint",
        "test": "pnpm test"
      },
      "output": {
        "build": ".next",
        "standalone": ".next/standalone"
      },
      "envSources": [".env.local", "Vercel project settings", "Supabase project"],
      "notes": [
        "Uses next-pwa for service worker bundling when available.",
        "Relies on Supabase client and server SDKs.",
        "Contains edge/runtime API routes that require Node-compatible environment."
      ]
    }
  ],
  "packages": [
    {
      "name": "supabase",
      "path": "supabase",
      "type": "configuration",
      "notes": "Contains Supabase migrations, seed scripts, and generated client types."
    }
  ],
  "infrastructure": {
    "vercelConfig": "vercel.json",
    "dockerfile": true,
    "ci": {
      "existingWorkflows": [".github/workflows"]
    }
  }
}
