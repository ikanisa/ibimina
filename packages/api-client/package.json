{
  "name": "@ibimina/api-client",
  "version": "1.0.0",
  "description": "Unified API client for Ibimina platform with Supabase integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["api", "client", "supabase"],
  "author": "Ibimina Team",
  "license": "MIT",
  "dependencies": {
    "@ibimina/types": "workspace:*",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
