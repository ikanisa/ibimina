{
  "cli": {
    "version": ">=3.20.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "credentialsSource": "remote",
      "env": {
        "EAS_PROJECT_ID": "${EAS_PROJECT_ID_SACCO}",
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL_SACCO}"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "internal": {
      "distribution": "internal",
      "channel": "internal",
      "credentialsSource": "remote",
      "env": {
        "EAS_PROJECT_ID": "${EAS_PROJECT_ID_SACCO}",
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL_SACCO}"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true,
      "credentialsSource": "remote",
      "env": {
        "EAS_PROJECT_ID": "${EAS_PROJECT_ID_SACCO}",
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL_SACCO}"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "${EAS_SUBMIT_IOS_APP_ID_SACCO}"
      },
      "android": {
        "serviceAccountKeyPath": "./credentials/google-play-service-account.json",
        "track": "internal"
      }
    }
  }
}
