{
  "name": "@ibimina/admin",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "build": "next build",
    "build:cloudflare": "CLOUDFLARE_BUILD=1 npx @cloudflare/next-on-pages",
    "preview:cloudflare": "wrangler pages dev .vercel/output/static",
    "deploy:cloudflare": "wrangler pages deploy .vercel/output/static",
    "start": "next start -p 3000 -H 0.0.0.0",
    "lint": "node ../../config/tooling/scripts/run-eslint.mjs .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:tests": "tsc -p tsconfig.tests.json --noEmit",
    "check:i18n": "node scripts/check-i18n.mjs",
    "check:i18n:consistency": "node scripts/check-i18n-consistency.mjs",
    "fix:i18n": "node scripts/fix-i18n.mjs && node scripts/check-i18n.mjs",
    "check:lighthouse": "npx --yes lighthouse http://localhost:3000 --view",
    "analyze:pwa": "lighthouse https://localhost:3000 --only-categories=pwa --preset=desktop || true",
    "verify:pwa": "node scripts/verify-pwa.mjs",
    "assert:lighthouse": "node scripts/assert-lighthouse.mjs",
    "analyze:bundle": "node scripts/analyze-bundle.mjs",
    "assert:bundle": "node scripts/assert-bundle-budgets.mjs",
    "test": "pnpm run test:rls",
    "test:rls": "bash scripts/test-rls.sh",
    "test:rls:docker": "bash scripts/test-rls-docker.sh",
    "test:auth": "pnpm exec c8 --clean=false --temp-directory=.nyc_output --report-dir=coverage/auth --reporter=lcov --reporter=json-summary --reporter=text-summary tsx --test tests/integration/authx-security.test.ts",
    "test:unit": "pnpm exec c8 --clean=false --temp-directory=.nyc_output --report-dir=coverage/unit --reporter=lcov --reporter=json-summary --reporter=text-summary tsx --test tests/unit/*.test.ts",
    "start:e2e": "node scripts/start-e2e.mjs",
    "verify:log-drain": "tsx scripts/verify-log-drain.ts",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/device": "^7.0.2",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.3",
    "@ibimina/config": "workspace:*",
    "@ibimina/flags": "workspace:*",
    "@ibimina/locales": "workspace:*",
    "@ibimina/ui": "workspace:*",
    "@openai/agents": "^0.1.11",
    "@sentry/nextjs": "^10.22.0",
    "@sentry/types": "^8.37.1",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@simplewebauthn/types": "^12.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.74.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "cmdk": "^1.1.0",
    "framer-motion": "^12.23.22",
    "idb": "^7.1.1",
    "lucide-react": "^0.545.0",
    "next": "15.5.2",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "openai": "^6.3.0",
    "papaparse": "^5.5.3",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sonner": "^2.0.7",
    "workbox-cacheable-response": "^6.6.0",
    "workbox-core": "^6.6.0",
    "workbox-expiration": "^6.6.0",
    "workbox-precaching": "^6.6.0",
    "workbox-routing": "^6.6.0",
    "workbox-strategies": "^6.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^14.2.13",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "pnpm@9.12.3"
}
